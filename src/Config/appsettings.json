{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\revs_bens_service_service\\log-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ElasticSearchAwsConfiguration": {
    "Region": "eu-west-1",
    "IndexFormat": "revs-bens-service-local-{0:yyyy.MM-dd}",
    "InlineFields": false,
    "MinimumLevel": "Debug",
    "Enabled": false,
    "Url": "https://search-stockport-logs-4evarozpqvlofjsnxzliq7eqdy.eu-west-1.es.amazonaws.com/"
  },
  "Availability": {
    "BaseUrl": "http://scnavailprod.stockport.gov.uk/api/v1",
    "ErrorRoute": "/error/500",
    "WhitelistedRoutes": [
      "/swagger/index.html"
    ],
    "Environment": "local",
    "AllowSwagger": true
  },
  "HttpClientConfiguration": [
    {
      "iGatewayType": "StockportGovUK.NetStandard.Gateways.CivicaServiceGateway.ICivicaServiceGateway",
      "gatewayType": "StockportGovUK.NetStandard.Gateways.CivicaServiceGateway.CivicaServiceGateway, StockportGovUk.NetStandard.Gateways",
      "baseUrl": "http://scnwebdev1.stockport.gov.uk/civicaservice/"
    }
  ]
}